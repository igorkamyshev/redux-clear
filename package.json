{
  "name": "redux-clear",
  "version": "1.1.3",
  "main": "dist/index.js",
  "repository": "git@github.com:igorkamyshev/redux-clear.git",
  "author": "Igor Kamyshev <igor@kamyshev.me>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepare": "install-self-peers -- --ignore-scripts && yarn build",
    "ci": "yarn types && yarn test && yarn s lint && yarn size",
    "test": "jest",
    "types": "tsc --noEmit",
    "size": "yarn build && size-limit",
    "s": "yarn soda"
  },
  "devDependencies": {
    "@solid-soda/scripts": "^1.9.2",
    "@team-griffin/install-self-peers": "^1.1.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.19",
    "jest": "^24.0.0",
    "rimraf": "^2.6.3",
    "size-limit": "^1.3.5",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  },
  "size-limit": [
    {
      "path": "dist/index.js"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "redux": "^4.0.1"
  },
  "dependencies": {
    "nanoid": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn soda lint-staged",
      "commit-msg": "yarn soda commitlint"
    }
  }
}
